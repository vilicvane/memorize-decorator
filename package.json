  "dependencies": {
    "multikey-map": "^0.2.1",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@mufan/eslint-plugin": "^0.2.16",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "run-in-every": "^0.2.0",
    "typescript": "^5.2.2",
    "yarn-deduplicate": "^6.0.2"
  }
